		"@reduxjs/toolkit": "^1.7.1",
		"@rematch/core": "^2.2.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"@types/jest": "^26.0.24",
		"@types/node": "^17.0.5",
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"babel-loader": "^8.2.3",
		"node-sass": "^6.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.6",
		"react-router-dom": "6",
		"react-scripts": "4.0.3",
		"rebass": "^4.0.7",
		"redux": "^4.1.2",
		"redux-devtools-extension": "^2.13.9",
